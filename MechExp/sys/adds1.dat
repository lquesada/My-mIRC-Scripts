dialog socktelnet {
  title $ø(112)
  size -1 -1 200 106
  option pixels
  icon sys\ico\Telnet.ico, 0
  edit $_res(ips), 1, 60 18 130 20,autohs,limit300
  edit , 2, 60 42 130 20,limit5
  box $ø(117), 6, 2 2 196 72
