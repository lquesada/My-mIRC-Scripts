    window -a @Logs
    _listlogs
  }
  else window -a @Logs
}
menu @Logs {
  dclick:{
    _loadlog
  }
  $ø(6):run user\logs
  -
  $ø(270):_listlogs
  $iif($sline(@Logs,1),$ø(272)):_loadlog
  -
  $ø(121):clear
}
alias -l _listlogs {
  clear @Logs
  clear -l @Logs
  echo $color(normal) @Logs $ø(273)
  var %x = $findfile(user\logs\,*.log,0,0,aline -l @Logs $nopath($left($1-,-4)))
  if (%x) echo $color(normal) @Logs $ø(274)
  else echo $color(normal) @Logs $ø(271)
}
alias -l _loadlog {
  if ($exists(user\logs\ $+ $sline(@Logs,1) $+ .log)) {
    clear @Logs
    loadbuf @Logs user\logs\ $+ $sline(@Logs,1) $+ .log
  }
}
