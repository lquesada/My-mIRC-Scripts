alias _telnetd _dialog socktelnet d
on *:DIALOG:socktelnet:init:0:_set stINIT 0 | did -a socktelnet 2 $gettok($_res(ports),1,61)
on *:DIALOG:socktelnet:edit:1,2:{
  if (!$_ret(stINIT)) { _set stINIT 1 | did -r socktelnet 1,2 }
  if ($did == 2) {
    if ($_solonumt($did(2),65535) != $did(2)) {
      did -r socktelnet 2
      if ($ifmatch) did -a socktelnet 2 $ifmatch
    }
  }
}
on *:DIALOG:socktelnet:mouse:*:{
  if (($mouse.x >= 60) && ($mouse.x <= 190) && ($mouse.y >= 17) && ($mouse.y <= 59)) {
    if (!$_ret(stINIT)) { 
      _set stINIT 1
      did -r socktelnet 1,2
    }
  }
}
alias -l _solonumt {
  var %x = 1,%y 
  while (%x <= $len($1)) { if (($asc($mid($1,%x,1)) >= 48) && ($asc($mid($1,%x,1)) <= 57)) var %y = %y $+ $chr($ifmatch) | inc %x }
  return $iif(%y > $2,$2,%y)
}
on *:DIALOG:socktelnet:sclick:*:{
  if ($did == 4) {
    if (!$_ret(stINIT)) { did -r socktelnet 1,2 | return }
    else {
      if ((!$did(1)) || (!$did(2)) || ($did(2) !isnum)) return
      telnet $did(1) $did(2)
    }
  }
}

alias -l _ttitle if ($window($1)) titlebar $1 $+([,$iif($sock($1),$sock($1).mark $sock($1).status,$ø(116)),])
alias telnet {
  var %x,%y
  if ((: isin $1) && (!$2)) var %x = $gettok($1,1,58),%y = $gettok($1,2,58)
  elseif ($2 isnum) var %x = $1,%y = $2
  if ((!%x) || (!%y) || (%y !isnum)) { _error $ø(108) | return }
  var %z
  while (($sock(%z)) || ($window(%z)) || (!%z)) var %z = @telnet- $+ $rand(a,z) $+ $rand(a,z) $+ $rand(a,z)
  sockopen %z %x %y
  sockmark %z $+(%x,:,%y)
  window -aek %z @telnet sys\ico\telnet.ico
  echo $color(info) %z $Ø(&t) * $ø(110) $+(%x,:,%y)
  _ttitle %z
}
on *:SOCKOPEN:@telnet-*:{
  var %x = $sockname
  if ($sockerr) {
    echo $color(info) %x $Ø(&t) * $sock(%x).wsmsg
    sockclose %x
  }
  else echo $color(info) %x $Ø(&t) * $ø(111) $sock($sockname).mark
  _ttitle %x
}
on *:CLOSE:@telnet-*:{ 
  if ($sock($target)) {
    echo $color(info) -s $Ø(&t) $ø(109) $sock($target).mark
    sockclose $target
  }
}
on *:INPUT:@telnet-*:{
  if ($sock($active)) {
    echo $color(normal) -a $Ø(&t) $iif($_cfgret(bsend),Bin)  $+ $color(own) $+ > $1-
    sockwrite $+(-t,$iif(!$_cfgret(bsend),n)) $active $1-
  }
  else {
    beep
    echo $color(normal) -a $Ø(&t) $ø(115)
  }
  halt
}
on *:SOCKREAD:@telnet-*:{
  if ($sockerr) {
    echo $color(info) $sockname $Ø(&t) * $sock($sockname).wsmsg
    sockclose $sockname
    return
  }
  sockread &d
  var %x = 1,%y
  while (%x <= $bvar(&d,0)) {
    if (($bvar(&d,%x) == 13) && ($bvar(&d,$calc(%x +1)) == 10)) {
      inc %x 2
      echo $color(normal) $sockname $Ø(&t)  $+ $color(info) $+ > %y
      var %y
    }
    %y = $iif($bvar(&d,%x) == 32,%y $chr(32),$+(%y,$chr($bvar(&d,%x))))
    if ($len(%y) == 900) {
      echo $color(normal) $sockname $Ø(&t)  $+ $color(info) $+ > %y
      var %y
    }
    inc %x
  }
  if (%y) echo $color(normal) $sockname $Ø(&t) Bin  $+ $color(info) $+ > %y
}
on *:SOCKCLOSE:@telnet-*:{
  var %x = $sockname
  sockclose %x
  _ttitle %x
  echo $color(info) %x $Ø(&t) * $ø(114)
}
menu @telnet {
  $ø(117):telnet 0
  $iif(!$sock($active),$style(2)) $ø(118):if ($sock($active)) { echo $color(info) -a $Ø(&t) * $ø(109) $sock($active).mark | sockclose $active | _ttitle $active }
  -
  $iif($sock($active).status != active,$style(2)) $ø(119):{
    if ($sock($active).status == active) { echo $color(normal) -a $Ø(&t)  $+ $color(own) $+ > | sockwrite -nt $active }
    else { beep | echo $color(normal) -a $Ø(&t) $ø(115) }
  }
  -
  $iif($_cfgret(bsend),$style(1)) $ø(120):_cfgset bsend $iif($_cfgret(bsend),0,1)
  -
  $ø(121):clear $active
}


